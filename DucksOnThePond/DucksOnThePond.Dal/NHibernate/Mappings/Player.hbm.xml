<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="DucksOnThePond.Core.Models" assembly="DucksOnThePond.Core">
  <class name="Player" table="Player" dynamic-update="true">
    <cache usage="read-write" />
    <id name="Id" column="Id" type="int">
      <generator class="native" />
    </id>
    <property name="Number" />
    <property name="FirstName" />
    <property name="LastName" />
  </class>
</hibernate-mapping>